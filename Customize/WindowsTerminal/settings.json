{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": [
        // ========== START NEW WINDOWS ==========
        {
            "command": {
                "action": "newTab"
            },
            "keys": "ctrl+shift+t"
        },
        {
            "command": "closeWindow",
            "keys": "alt+f4"
        },
        {
            "command": "openTabRenamer",
            "keys": "alt+shift+q"
        },
        {
            "command": {
                "action": "splitPane",
                "split": "right",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+plus"
        },
        {
            "command": {
                "action": "splitPane",
                "split": "down",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+minus"
        },
        {
            "command": "openNewTabDropdown",
            "keys": "ctrl+shift+space"
        },
        {
            "command": {
                "action": "newTab",
                "index": 0
            },
            "keys": "ctrl+shift+1"
        },
        {
            "command": {
                "action": "newTab",
                "index": 1
            },
            "keys": "ctrl+shift+2"
        },
        {
            "command": {
                "action": "newTab",
                "index": 2
            },
            "keys": "ctrl+shift+3"
        },
        {
            "command": {
                "action": "newTab",
                "index": 3
            },
            "keys": "ctrl+shift+4"
        },
        {
            "command": {
                "action": "newTab",
                "index": 4
            },
            "keys": "ctrl+shift+5"
        },
        {
            "command": {
                "action": "newTab",
                "index": 5
            },
            "keys": "ctrl+shift+6"
        },
        {
            "command": {
                "action": "newTab",
                "index": 6
            },
            "keys": "ctrl+shift+7"
        },
        {
            "command": {
                "action": "newTab",
                "index": 7
            },
            "keys": "ctrl+shift+8"
        },
        {
            "command": {
                "action": "newTab",
                "index": 8
            },
            "keys": "ctrl+shift+9"
        },
        {
            "command": 
            {
                "action": "multipleActions",
                "actions": 
                [
                    {
                        "action": "splitPane",
                        "size":0.1,
                        "split": "up",
                        "splitMode":"duplicate"
                    },
                    {
                        "action": "moveFocus",
                        "direction": "previous"
                    },
                    {
                        "action": "closePane"
                    }
                ]
            },
            "keys": "ctrl+shift+r",
            "name": "restart tab"
        },
        // ========== END NEW WINDOWS ==========
        // ========== START NAVIGATE WINDOWS ACTIVATE ==========
        // START NAVIGATE PANEL WINDOWS ACTIVATE 
        {
            "command": {
                "action": "moveFocus",
                "direction": "left"
            },
            "keys": "alt+h"
        },
        {
            "command": {
                "action": "moveFocus",
                "direction": "down"
            },
            "keys": "alt+j"
        },
        {
            "command": {
                "action": "moveFocus",
                "direction": "up"
            },
            "keys": "alt+k"
        },
        {
            "command": {
                "action": "moveFocus",
                "direction": "right"
            },
            "keys": "alt+l"
        },
        // END NAVIGATE PANEL WINDOWS ACTIVATE 
        // START RESIZE PANEL
        {
            "command": {
                "action": "resizePane",
                "direction": "left"
            },
            "keys": "alt+shift+h" 
        },
        {
            "command": {
                "action": "resizePane",
                "direction": "down"
            },
            "keys": "alt+shift+j"
        },
        {
            "command": {
                "action": "resizePane",
                "direction": "right"
            },
            "keys": "alt+shift+l"
        },
        {
            "command": {
                "action": "resizePane",
                "direction": "up"
            },
            "keys": "alt+shift+k"
        },
        // END RESIZE PANEL
        {
            "command": {
                "action": "moveTab",
                "direction": "backward"
            },
            "keys": "ctrl+alt+["
        },
        {
            "command": {
                "action": "moveTab",
                "direction": "forward"
            },
            "keys": "ctrl+alt+]"
        },
        {
            "command": "toggleReadOnlyMode",
            "keys": "alt+shift+r"
        },
        // END SCROLL COMMANDLINE WINDOWS ACTIVATE
        {
            "command": {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        },
        {
            "command": "togglePaneZoom",
            "keys": "alt+shift+z"
        },
        { "command": "toggleFocusMode", "keys": "alt+shift+f"},
        // ========== NAVIGATE WINDOWS ACTIVATE ==========
        // ========== OTHER FEATURES ==========
        {
            "command": {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+shift+c"
        },
        {
            "command": "paste",
            "keys": "ctrl+shift+v"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": "tabSearch",
            "keys": "ctrl+alt+f"
        }
        // { "command": { "action": "sendInput", "input": "\b" }, "keys": "backspace" }
        // { "command": { "action": "sendInput", "input": "\u001b\u007f" }, "keys": "ctrl+backspace" }
    ],
    "alwaysShowTabs": true,
    "alwaysOnTop": false,
    "copyFormatting": "none",
    "copyOnSelect": true ,
    "defaultProfile": "{61c54bbd-c2c6-5271-96e7-009a87ff44bf}",
    "disableAnimations": true,
    "language": "en-US",
    "launchMode": "default",
    "profiles": {
        "defaults": {
            "cursorShape": "vintage",
            "font": {
                "face": "Cascadia Code PL",
                "size": 9
            },
            "padding": "5, 5, 5, 5",
            "backgroundImage": "https://raw.githubusercontent.com/0xTRAW/Linux-Commands-A-Z/main/Linux%20commands.png",
			"backgroundImageStretchMode": "uniform",
			"backgroundImageOpacity": 0.3,
			"backgroundImageAlignment": "top"
        },
        "list": [
            {
                "name": "Windows PowerShell",
                "colorScheme": "Blue Matrix",
                "cursorColor": "#FFFFFF",
                "hidden": false,
                "suppressApplicationTitle": true
            },
            {
                "name": "Command Prompt",
                "colorScheme": "Retro",
                "commandline": "cmd.exe",
                "cursorShape": "bar",
                "hidden": true ,
                "suppressApplicationTitle": true
            }
        ]
    },
