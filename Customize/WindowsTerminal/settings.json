{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
    // ========== NAVIGATION WINDOWS ==========
        {
            "command": 
            {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+shift+c"
        },
        {
            "command": "openTabRenamer",
            "keys": "alt+q"
        },
        {
            "command": "paste",
            "keys": "ctrl+shift+v"
        },
        {
            "command": 
            {
                "action": "newTab"
            },
            "keys": "ctrl+shift+t"
        },
        {
            "command": "closeWindow",
            "keys": "alt+f4"
        },
        {
            "command": "openNewTabDropdown",
            "keys": "ctrl+shift+space"
        },
        
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        
        {
            "command": 
            {
                "action": "newTab",
                "index": 0
            },
            "keys": "ctrl+shift+1"
        },
        {
            "command": 
            {
                "action": "newTab",
                "index": 1
            },
            "keys": "ctrl+shift+2"
        },
        {
            "command": 
            {
                "action": "newTab",
                "index": 2
            },
            "keys": "ctrl+shift+3"
        },
        {
            "command": 
            {
                "action": "newTab",
                "index": 3
            },
            "keys": "ctrl+shift+4"
        },
        {
            "command": 
            {
                "action": "newTab",
                "index": 4
            },
            "keys": "ctrl+shift+5"
        },
        {
            "command": 
            {
                "action": "newTab",
                "index": 5
            },
            "keys": "ctrl+shift+6"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "previous"
            },
            "keys": "ctrl+alt+h"
        },
        {
            "command": 
            {
                "action": "newTab",
                "index": 7
            },
            "keys": "ctrl+shift+8"
        },
        {
            "command": 
            {
                "action": "newTab",
                "index": 6
            },
            "keys": "ctrl+shift+7"
        },
        {
            "command": 
            {
                "action": "newTab",
                "index": 8
            },
            "keys": "ctrl+shift+9"
        },
        {
            "command": "tabSearch",
            "keys": "ctrl+alt+f"
        },
        {
            "command": 
            {
                "action": "moveTab",
                "direction": "backward"
            },
            "keys": "ctrl+alt+["
        },
        {
            "command": 
            {
                "action": "moveTab",
                "direction": "forward"
            },
            "keys": "ctrl+alt+]"
        },
        {
            "command": "toggleReadOnlyMode",
            "keys": "alt+shift+r"
        },
        {
            "command": 
            {
                "action": "sendInput",
                "input": "cd ..\rls\r"
            },
            "keys": "alt+ctrl+h"
        },
        {
            "command": 
            {
                "action": "sendInput",
                "input": "clear\r"
            },
            "keys": "ctrl+shift+k"
        },
        { "command": "scrollUp", "keys": "ctrl+k" },
        { "command": "scrollDown", "keys": "ctrl+j" },
        {
            "command": 
            {
                "action": "sendInput",
                "input": "ls\r"
            },
            "keys": "ctrl+l"
        },
    // ========== WINDOWS PANE - NEW PANE ==========
    {
            "command": 
            {
                "action": "splitPane",
                "split": "right"
            },
            "keys": "alt+shift+plus"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "down"
            },
            "keys": "alt+shift+minus"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        },
        {
            "command": "togglePaneZoom",
            "keys": "alt+shift+z"
        },
    // ========== WINDOWS PANE - NAVIGATE PANE - Alt + H/J/K/L ==========
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "left"
            },
            "keys": "alt+h"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "down"
            },
            "keys": "alt+j"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "up"
            },
            "keys": "alt+k"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "right"
            },
            "keys": "alt+l"
        },
    // ========== WINDOWS PANE - RESIZE PANE - ALT + SHIFT + H/J/K/L ==========
        {
            "command": 
            {
                "action": "resizePane",
                "direction": "left"
            },
            "keys": "alt+shift+h"
        },
        {
            "command": 
            {
                "action": "resizePane",
                "direction": "down"
            },
            "keys": "alt+shift+j"
        },

        {
            "command": 
            {
                "action": "resizePane",
                "direction": "right"
            },
            "keys": "alt+shift+l"
        },
        {
            "command": 
            {
                "action": "resizePane",
                "direction": "up"
            },
            "keys": "alt+shift+k"
        },

        
    ],
